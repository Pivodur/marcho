         <div class="top__container">
            <h2 class="title top__title">
               SHOP PAGE
            </h2>
            <div class="breadcrumbs">
               <ul class="breadcrumbs__list">
                  <li class="breadcrumbs__list-item">
                     <a class="breadcrumbs__list-link" href="#">
                        Home
                     </a>
                  </li>
                  <li class="breadcrumbs__list-item">
                     <span class="breadcrumbs__list-span">
                        Shop page
                     </span>
                  </li>
               </ul>
            </div>
         </div>
      </section>
      <section class="shop">
         <div class="container">
            <div class="shop__inner">
               <div class="filter shop__filter">
                  <div class="filter__item filter-search">
                     <h4 class="filter__title">
                        SEARCH
                     </h4>
                     <form class="filter-search__form" action="#">
                        <input class="filter-search__input" type="text" placeholder="Search your keyword....">
                        <button class="filter-search__btn" type="submit">
                           <img src="images/icons/search-white.svg" alt="icon search">
                        </button>
                     </form>
                  </div>
                  <div class="filter__item fitler-price">
                     <h4 class="filter__title">
                        PRICE FILTER
                     </h4>
                     <form class="filter-price__form" action="#">
                        <input class="filter-price__input" type="text" data-min="0" data-max="999" data-from="200"
                           data-to="800">
                        <label class="filter-price__label">
                           <span class="filter-price__range">
                              Price:
                              $<span class="filter__price-from"></span> -
                              $<span class="filter__price-to"></span>
                           </span>
                           <button class="filter-price__btn">FILTER</button>
                        </label>
                     </form>
                  </div>
                  <div class="filter__item filter-color">
                     <h4 class="filter__title">
                        COLOR FILTER
                     </h4>
                     <form class="filter-color__form" action="#">
                        <label class="filter-color__label">
                           <span class="filter-color__color">Blue (15)</span>
                           <div class="filter-color__box">
                              <input class="filter-color__input" type="checkbox">
                              <span class="filter-color__checkbox filter-color__checkbox--blue"></span>
                           </div>
                        </label>
                        <label class="filter-color__label">
                           <span class="filter-color__color">Red (09)</span>
                           <div class="filter-color__box">
                              <input class="filter-color__input" type="checkbox">
                              <span class="filter-color__checkbox filter-color__checkbox--red"></span>
                           </div>
                        </label>
                        <label class="filter-color__label">
                           <span class="filter-color__color">Green (28)</span>
                           <div class="filter-color__box">
                              <input class="filter-color__input" type="checkbox">
                              <span class="filter-color__checkbox filter-color__checkbox--green"></span>
                           </div>
                        </label>
                        <label class="filter-color__label">
                           <span class="filter-color__color">Orange (11)</span>
                           <div class="filter-color__box">
                              <input class="filter-color__input" type="checkbox">
                              <span class="filter-color__checkbox filter-color__checkbox--orange"></span>
                           </div>
                        </label>
                        <label class="filter-color__label">
                           <span class="filter-color__color">Black (05)</span>
                           <div class="filter-color__box">
                              <input class="filter-color__input" type="checkbox">
                              <span class="filter-color__checkbox filter-color__checkbox--black"></span>
